<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/fragEmpresa"
    tools:context=".fragEmpresa">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center_horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:textSize="18sp"
            android:layout_gravity="left"
            android:gravity="left"
            android:text=" Registro de Empresa" />

        <EditText
            android:id="@+id/editempre"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:ems="100"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:inputType="textPersonName"
            android:text="Empresa/Urbanizacion" />

        <EditText
            android:id="@+id/editdirem"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:ems="100"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:inputType="textPersonName"
            android:text="Direccion" />

        <EditText
            android:id="@+id/editmail"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:ems="100"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:inputType="textPersonName"
            android:text="mail" />

        <EditText
            android:id="@+id/editncell"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:ems="100"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:inputType="textPersonName"
            android:text="Telefono" />

        <EditText
            android:id="@+id/editdni"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:ems="100"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:inputType="textPersonName"
            android:text="DNI/RUC/Pasp" />

        <EditText
            android:id="@+id/editnadm"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:ems="100"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:inputType="textPersonName"
            android:text="Administrador" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="bottom"
            android:gravity="center"
            >
            <Button
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_horizontal"
                android:text=" Grabar "
                android:textSize="18sp">
            </Button>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_horizontal"
                android:text="Regresar"
                android:textSize="18sp">
            </Button>
        </LinearLayout>

    </LinearLayout>
</FrameLayout>